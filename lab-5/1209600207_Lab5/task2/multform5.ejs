<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Hair Color</title>
</head>
<body>
 Hair Color: <br>
  <form action="multform6" method="POST">
    <select name="hairColor">
    <option value="">Select Hair Color</option>
    <% hairColor_list.forEach(function(hairColor){ %>
      <% if(selectedHairColors && selectedHairColors.indexOf(hairColor) > -1) { %>
      <option selected="selected" value="<%= hairColor %>" /><%= hairColor %></option>
      <% } else { %>
      <option value="<%= hairColor %>" /><%= hairColor %></option>
      <% } %>
    <% }); %>
    <br>
  <input type="submit" value="Next">
  </form>

  <form action="multform4" method="POST">
  <input type="submit" value="Previous">
  </form>

</body>